; Like (modulo p q), but works with non-integer numbers
(: mod (number number -> number))
(define mod
  (lambda (p q)
    (- p (* q (floor (/ p q))))))

; Build a list of increasing numbers from s to e
(: from-to (number number -> (list-of number)))
(define from-to
  (lambda (s e)
    (if (> s e)
        empty
        (make-pair s (from-to (+ s 1) e)))))

